<view class="content"><view class="logo"><image class="img" mode="widthFix" src="{{mallLogo}}"></image></view><block wx:if="{{showOauthBtn}}"><view class="oauth-row"><view class="oauth-btn"><button class="oauth-btn" type="primary" open-type="getUserInfo" data-event-opts="{{[['getuserinfo',[['onGotUserInfo',['$event']]]]]}}" bindgetuserinfo="__e">微信登陆</button></view></view></block><block wx:if="{{isPhoneLogin}}"><view class="web-row"><view class="input-group"><view class="input-row"><m-input bind:input="__e" class="m-input" vue-id="1" type="number" clearable focus placeholder="输入手机号" value="{{phoneData.phone}}" data-event-opts="{{[['^input',[['__set_model',['$0','phone','$event',[]],['phoneData']]]]]}}" bind:__l="__l"></m-input></view><view class="input-row"><m-input bind:input="__e" class="m-input input-code" vue-id="2" type="text" placeholder="输入验证码" value="{{phoneData.code}}" data-event-opts="{{[['^input',[['__set_model',['$0','code','$event',[]],['phoneData']]]]]}}" bind:__l="__l"></m-input><block wx:if="{{!timeTick}}"><button class="btn-code" type="primary" data-event-opts="{{[['tap',[['sendCode',['$event']]]]]}}" bindtap="__e">发送验证码</button></block><block wx:if="{{timeTick}}"><button class="btn-code" type="primary">{{"剩余"+countdown+"秒"}}</button></block></view></view><view class="btn-row"><button class="web-btn" type="primary" data-event-opts="{{[['tap',[['handlePhoneLogin',['$event']]]]]}}" bindtap="__e">登录</button></view></view></block><block wx:if="{{isPasswordLogin}}"><view class="web-row"><view class="input-group"><view class="input-row"><m-input bind:input="__e" class="m-input" vue-id="3" type="number" clearable focus placeholder="输入手机号" value="{{passwordData.loginName}}" data-event-opts="{{[['^input',[['__set_model',['$0','loginName','$event',[]],['passwordData']]]]]}}" bind:__l="__l"></m-input></view><view class="input-row"><m-input bind:input="__e" class="m-input" vue-id="4" type="text" placeholder="请输入密码" value="{{passwordData.password}}" data-event-opts="{{[['^input',[['__set_model',['$0','password','$event',[]],['passwordData']]]]]}}" bind:__l="__l"></m-input></view></view><view class="btn-row"><button class="web-btn" type="primary" data-event-opts="{{[['tap',[['handlePasswordLogin',['$event']]]]]}}" bindtap="__e">登录</button></view></view></block></view>