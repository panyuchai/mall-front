<view><search class="search-bar" vue-id="1" keyword="{{searchData.keyword}}" bind:__l="__l"></search><view class="search-classify"><view class="classify-top"><view data-event-opts="{{[['tap',[['doQuery',[0]]]]]}}" class="{{['item',searchData.orderType==0?'red':'']}}" bindtap="__e">综合</view><view data-event-opts="{{[['tap',[['doQuery',[1]]]]]}}" class="{{['item',searchData.orderType==1?'red':'']}}" bindtap="__e">最新</view><view data-event-opts="{{[['tap',[['doQuery',[2]]]]]}}" class="item" bindtap="__e">价格<text class="item-icon"><text class="{{['iconfont icon-icon_sanjiaoxing icon-top',searchData.orderType==2&&searchData.desc?'red':'']}}"></text><text class="{{['iconfont icon-icon_sanjiaoxing icon-bottom',searchData.orderType==2&&!searchData.desc?'red':'']}}"></text></text></view><view data-event-opts="{{[['tap',[['doQuery',[3]]]]]}}" class="item" bindtap="__e">销量<text class="item-icon"><text class="{{['iconfont icon-icon_sanjiaoxing icon-top',searchData.orderType==3&&searchData.desc?'red':'']}}"></text><text class="{{['iconfont icon-icon_sanjiaoxing icon-bottom',searchData.orderType==3&&!searchData.desc?'red':'']}}"></text></text></view></view></view><block wx:if="{{notFound}}"><view class="product-empty"><view class="not-tips">没有找到相关搜索结果</view></view></block><view class="product-list" scroll-top="{{scrollTop}}" data-event-opts="{{[['scroll',[['scroll',['$event']]]]]}}" bindscroll="__e"><block wx:for="{{productData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['linkToDetail',['$0'],[[['productData','',index,'goodspoolId']]]]]]]}}" class="pro-item" bindtap="__e"><image class="pro-img" src="{{item.goodsMainimagepath}}" mode="widthFix"></image><view class="pro-content"><view class="pro-tit">{{item.goodsProductname}}</view><view><view class="pro-subtit">{{''+item.goodsTitle+''}}</view><view class="salePrice"><view class="rulingPrice"><block wx:if="{{item.credits}}"><view class="price"><text class="text">{{item.credits}}</text>积分</view></block><block wx:if="{{item.salePrice&&item.credits}}"><view>+</view></block><block wx:if="{{item.salePrice}}"><view class="price"><text class="text">{{item.salePrice}}</text>元</view></block></view><view class="markedPrice">{{''+item.goodsVirtualprice+'元'}}</view></view></view></view><block wx:if="{{item.label}}"><view class="tag">{{''+item.label+''}}</view></block></view></block></view><tui-loadmore vue-id="2" visible="{{loadding}}" index="{{3}}" type="primary" bind:__l="__l"></tui-loadmore><tui-nomore vue-id="3" visible="{{!pullUpOn}}" bind:__l="__l"></tui-nomore></view>