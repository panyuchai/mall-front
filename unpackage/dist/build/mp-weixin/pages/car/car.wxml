<view><view class="blank"></view><block><block wx:if="{{hasLogin}}"><view><view class="goods-list"><block wx:if="{{carEmpty}}"><view class="product-empty"><view class="not-tips">购物车是空的哦~</view></view></block><block wx:for="{{goodsList}}" wx:for-item="row" wx:for-index="index" wx:key="index"><view class="row"><view data-event-opts="{{[['tap',[['deleteGoods',['$0'],[[['goodsList','',index,'shopcarId']]]]]]]}}" class="menu" catchtap="__e"><view class="icon iconfont icon-shanchu"></view></view><view data-event-opts="{{[['touchstart',[['touchStart',[index,'$event']]]],['touchmove',[['touchMove',[index,'$event']]]],['touchend',[['touchEnd',[index,'$event']]]]]}}" class="{{['carrier',theIndex==index?'open':oldIndex==index?'close':'']}}" bindtouchstart="__e" bindtouchmove="__e" bindtouchend="__e"><view data-event-opts="{{[['tap',[['selected',[index]]]]]}}" class="checkbox-box" bindtap="__e"><view class="{{['checkbox',row.checked?'on':'']}}"><view class="{{[row.checked?'on':'']}}"></view></view></view><view data-event-opts="{{[['tap',[['linkToDetail',['$0'],[[['goodsList','',index,'mallGoodsId']]]]]]]}}" class="goods-info" bindtap="__e"><view class="img"><image src="{{row.goodsMainimagepath}}"></image></view><view class="info"><view class="title">{{row.goodsProductname}}</view><view class="spec">{{row.goodsTitle}}</view><view class="price-number"><view class="price"><view class="sale-price">￥<text class="num">{{row.salePrice}}</text></view></view><view class="number"><view data-event-opts="{{[['tap',[['sub',[index]]]]]}}" class="sub" catchtap="__e"><view class="icon iconfont icon-jian"></view></view><view data-event-opts="{{[['tap',[['discard',['$event']]]]]}}" class="input" catchtap="__e"><input type="number" data-event-opts="{{[['input',[['__set_model',['$0','goodsCount','$event',[]],[[['goodsList','',index]]]],['sum',['$event',index]]]],['blur',[['change',[index]]]]]}}" value="{{row.goodsCount}}" bindinput="__e" bindblur="__e"/></view><view data-event-opts="{{[['tap',[['add',[index]]]]]}}" class="add" catchtap="__e"><view class="icon iconfont icon-jia"></view></view></view></view></view></view></view></view></block></view><view class="footer" style="{{'bottom:'+(footerbottom)+';'}}"><view data-event-opts="{{[['tap',[['allSelect',['$event']]]]]}}" class="checkbox-box" bindtap="__e"><view class="{{['checkbox',isAllselected?'on':'']}}"><view class="{{[isAllselected?'on':'']}}"></view></view><view class="text">全选</view></view><block wx:if="{{selectedList.length>0}}"><view data-event-opts="{{[['tap',[['deleteList',['$event']]]]]}}" class="delBtn" bindtap="__e">删除</view></block><view class="settlement"><view class="sum">价格:<view class="money">{{sumPrice+"元"}}</view></view><view data-event-opts="{{[['tap',[['linkToPayment',['$event']]]]]}}" class="btn" bindtap="__e">去结算</view></view></view></view></block><block wx:else><view class="product-empty"><view class="err-tips">您还未登陆，请先去<text data-event-opts="{{[['tap',[['linkToLogin',['$event']]]]]}}" class="login-btn" bindtap="__e">登陆</text>吧~</view></view></block></block></view>